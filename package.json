{
	"name": "shotcowboystyle.github.io",
	"description": "Personal site for Curtis Blanton. Hooray! âœ¨",
	"version": "0.0.0",
	"private": true,
	"type": "module",
	"author": {
		"name": "Curtis Blanton",
		"url": "https://shotcowboystyle.github.io"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/shotcowboystyle/shotcowboystyle.github.io"
	},
	"license": "MIT",
	"scripts": {
		"old:postinstall": "pnpm dlx lefthook install",
		"astro": "astro",
		"build": "astro build",
		"clean": "rm -rf .astro dist lighthouse node_modules pnpm-lock.yaml",
		"cz": "git-cz",
		"dev": "astro dev",
		"format:check": "prettier '**/*.{tsx,ts,mjs,cjs,js,json,css,yml,yaml,md,astro}' --check",
		"format:fix": "pnpm run format:check -w",
		"lint:check": "run-p --continue-on-error \"lint:*:check\"",
		"lint:fix": "run-p --continue-on-error \"lint:*:fix\"",
		"lint:code:check": "eslint --ext .js,.cjs,.mjs,.jsx,.ts,.tsx,.astro --ignore-path '.eslintignore' .",
		"lint:code:fix": "pnpm run lint:code:check --fix",
		"lint:package:check": "npmPkgJsonLint .",
		"lint:packages:check": "pnpm dedupe --check",
		"lint:knip:check": "knip --config knip.config.mjs",
		"lint:spelling:check": "cspell lint './**/*.{tsx,ts,mjs,cjs,js,json,css,yml,yaml,md,astro}' --gitignore --cache",
		"lint:styles:check": "stylelint src/**/*.{astro,css} --cache --ignore-path ./.gitignore",
		"lint:styles:fix": "pnpm run lint:styles:check --fix",
		"preview": "astro preview",
		"tsc": "tsc",
		"test": "playwright test e2e/tests/",
		"test:e2e": "run-s test:e2e:*",
		"test:e2e:accessibility": "playwright test e2e/tests/accessibility",
		"test:e2e:pages": "playwright test e2e/tests/pages",
		"test:e2e:performance": "playwright test e2e/tests/performance",
		"test:e2e:visual": "playwright test e2e/tests/visual",
		"test:playwright:codegen": "playwright codegen",
		"test:playwright:debug": "playwright test --debug",
		"test:playwright:ui": "playwright test --ui",
		"types:check": "astro sync && tsc --noEmit && astro check",
		"validate": "run-p format:check lint:check types:check test"
	},
	"dependencies": {
		"@astrojs/sitemap": "^3.0.5",
		"@astrojs/tailwind": "^5.1.0",
		"@builder.io/qwik": "^1.4.3",
		"@qwikdev/astro": "^0.5.2",
		"@studio-freight/lenis": "^1.0.34",
		"astro": "^4.2.1",
		"astro-svg-sprite": "^1.1.0",
		"astro-sw": "^1.0.4",
		"cannon": "^0.6.2",
		"gsap": "^3.12.5",
		"lottie-web": "^5.12.2",
		"maplibre-gl": "^3.6.2",
		"sharp": "^0.33.2",
		"split-type": "^0.3.4",
		"swiper": "^11.0.5",
		"three": "^0.161.0"
	},
	"devDependencies": {
		"@astrojs/check": "^0.4.1",
		"@astrojs/ts-plugin": "^1.4.0",
		"@axe-core/playwright": "^4.8.4",
		"@commitlint/cli": "^18.6.0",
		"@commitlint/config-conventional": "^18.6.0",
		"@ianvs/prettier-plugin-sort-imports": "^4.1.1",
		"@playwright/test": "^1.41.2",
		"@semantic-release/changelog": "^6.0.3",
		"@semantic-release/commit-analyzer": "^11.1.0",
		"@semantic-release/git": "10.0.1",
		"@semantic-release/github": "^9.2.6",
		"@semantic-release/release-notes-generator": "^12.1.0",
		"@types/cannon": "^0.1.12",
		"@types/eslint": "^8.56.2",
		"@types/three": "^0.161.2",
		"@typescript-eslint/eslint-plugin": "^6.20.0",
		"@typescript-eslint/parser": "^6.20.0",
		"astro-compress": "2.2.8",
		"astro-critters": "^2.0.11",
		"astro-eslint-parser": "^0.16.2",
		"astro-purgecss": "^4.1.0",
		"autoprefixer": "^10.4.17",
		"ci-info": "^4.0.0",
		"commitizen": "^4.3.0",
		"conventional-changelog-conventionalcommits": "^7.0.2",
		"cspell": "^8.3.2",
		"cssnano": "^6.0.3",
		"cz-conventional-changelog": "^3.3.0",
		"daisyui": "^4.6.1",
		"eslint": "^8.56.0",
		"eslint-import-resolver-typescript": "^3.6.1",
		"eslint-plugin-astro": "^0.31.3",
		"eslint-plugin-import": "^2.29.1",
		"eslint-plugin-jsx-a11y": "^6.8.0",
		"eslint-plugin-markdown": "^3.0.1",
		"knip": "^4.3.1",
		"lefthook": "1.6.1",
		"lighthouse": "^11.5.0",
		"npm-package-json-lint": "^7.1.0",
		"npm-package-json-lint-config-default": "^6.0.0",
		"npm-run-all2": "^6.1.2",
		"playwright": "^1.41.2",
		"playwright-lighthouse": "^4.0.0",
		"postcss": "^8.4.33",
		"postcss-combine-duplicated-selectors": "10.0.3",
		"postcss-for": "^2.1.1",
		"postcss-html": "^1.6.0",
		"postcss-import": "^16.0.0",
		"postcss-nesting": "^12.0.2",
		"postcss-preset-env": "^9.3.0",
		"postcss-random": "^1.0.9",
		"postcss-reporter": "7.1.0",
		"prettier": "^3.2.4",
		"prettier-plugin-astro": "^0.13.0",
		"prettier-plugin-tailwindcss": "^0.5.11",
		"purgecss": "^5.0.0",
		"rollup-plugin-analyzer": "^4.0.0",
		"rollup-plugin-visualizer": "^5.12.0",
		"semantic-release": "23.0.0",
		"stylelint": "^16.2.0",
		"stylelint-config-html": "^1.1.0",
		"stylelint-config-hudochenkov": "^10.0.0",
		"stylelint-config-standard": "^36.0.0",
		"stylelint-config-tailwindcss": "^0.0.7",
		"tailwindcss": "^3.4.1",
		"tailwindcss-fluid-type": "^2.0.3",
		"typescript": "^5.3.3"
	}
}
