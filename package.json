{
	"name": "shotcowboystyle.github.io",
	"description": "Personal site for Curtis Blanton. Hooray! âœ¨",
	"version": "0.0.0",
	"private": true,
	"type": "module",
	"author": {
		"name": "Curtis Blanton",
		"url": "https://shotcowboystyle.github.io"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/shotcowboystyle/shotcowboystyle.github.io"
	},
	"license": "MIT",
	"scripts": {
		"old:postinstall": "pnpm dlx lefthook install",
		"astro": "astro",
		"build": "astro build",
		"clean": "rm -rf .astro dist lighthouse node_modules pnpm-lock.yaml",
		"cz": "git-cz",
		"dev": "astro dev",
		"format:check": "prettier '**/*.{tsx,ts,mjs,cjs,js,json,css,yml,yaml,md,astro}' --check",
		"format:fix": "pnpm run format:check -w",
		"lint:check": "run-p --continue-on-error \"lint:*:check\"",
		"lint:fix": "run-p --continue-on-error \"lint:*:fix\"",
		"lint:code:check": "eslint --ext .js,.cjs,.mjs,.jsx,.ts,.tsx,.astro --ignore-path '.eslintignore' .",
		"lint:code:fix": "pnpm run lint:code:check --fix",
		"lint:package:check": "npmPkgJsonLint .",
		"lint:packages:check": "pnpm dedupe --check",
		"lint:knip:check": "knip --config knip.config.mjs",
		"lint:spelling:check": "cspell lint './**/*.{tsx,ts,mjs,cjs,js,json,css,yml,yaml,md,astro}' --gitignore --cache",
		"lint:styles:check": "stylelint src/**/*.{astro,css} --cache --ignore-path ./.gitignore",
		"lint:styles:fix": "pnpm run lint:styles:check --fix",
		"preview": "astro preview",
		"tsc": "tsc",
		"test": "playwright test e2e/tests/",
		"test:e2e": "run-s test:e2e:*",
		"test:e2e:accessibility": "playwright test e2e/tests/accessibility",
		"test:e2e:pages": "playwright test e2e/tests/pages",
		"test:e2e:performance": "playwright test e2e/tests/performance",
		"test:e2e:visual": "playwright test e2e/tests/visual",
		"test:playwright:codegen": "playwright codegen",
		"test:playwright:debug": "playwright test --debug",
		"test:playwright:ui": "playwright test --ui",
		"types:check": "astro sync && tsc --noEmit && astro check",
		"validate": "run-p format:check lint:check types:check test"
	},
	"dependencies": {
		"@astrojs/sitemap": "^3.7.0",
		"@astrojs/tailwind": "^6.0.2",
		"@builder.io/qwik": "^1.18.0",
		"@qwikdev/astro": "^0.8.2",
		"@studio-freight/lenis": "^1.0.42",
		"astro": "^5.16.11",
		"astro-svg-sprite": "^1.1.0",
		"astro-sw": "^1.0.5",
		"cannon": "^0.6.2",
		"gsap": "^3.14.2",
		"lottie-web": "^5.13.0",
		"maplibre-gl": "^5.16.0",
		"sharp": "^0.34.5",
		"split-type": "^0.3.4",
		"swiper": "^12.0.3",
		"three": "^0.182.0"
	},
	"devDependencies": {
		"@astrojs/check": "^0.9.6",
		"@astrojs/ts-plugin": "^1.10.6",
		"@axe-core/playwright": "^4.11.0",
		"@commitlint/cli": "^20.3.1",
		"@commitlint/config-conventional": "^20.3.1",
		"@ianvs/prettier-plugin-sort-imports": "^4.7.0",
		"@playwright/test": "^1.57.0",
		"@semantic-release/changelog": "^6.0.3",
		"@semantic-release/commit-analyzer": "^13.0.1",
		"@semantic-release/git": "10.0.1",
		"@semantic-release/github": "^12.0.2",
		"@semantic-release/release-notes-generator": "^14.1.0",
		"@types/cannon": "^0.1.13",
		"@types/eslint": "^9.6.1",
		"@types/three": "^0.182.0",
		"@typescript-eslint/eslint-plugin": "^8.53.1",
		"@typescript-eslint/parser": "^8.53.1",
		"astro-compress": "2.3.9",
		"astro-critters": "^2.2.1",
		"astro-eslint-parser": "^1.2.2",
		"astro-purgecss": "^5.3.0",
		"autoprefixer": "^10.4.23",
		"ci-info": "^4.3.1",
		"commitizen": "^4.3.1",
		"conventional-changelog-conventionalcommits": "^9.1.0",
		"cspell": "^9.6.0",
		"cssnano": "^7.1.2",
		"cz-conventional-changelog": "^3.3.0",
		"daisyui": "^5.5.14",
		"eslint": "^9.39.2",
		"eslint-import-resolver-typescript": "^4.4.4",
		"eslint-plugin-astro": "^1.5.0",
		"eslint-plugin-import": "^2.32.0",
		"eslint-plugin-jsx-a11y": "^6.10.2",
		"eslint-plugin-markdown": "^5.1.0",
		"knip": "^5.82.1",
		"lefthook": "2.0.15",
		"lighthouse": "^13.0.1",
		"npm-package-json-lint": "^9.1.0",
		"npm-package-json-lint-config-default": "^8.0.1",
		"npm-run-all2": "^8.0.4",
		"playwright": "^1.57.0",
		"playwright-lighthouse": "^4.0.0",
		"postcss": "^8.5.6",
		"postcss-combine-duplicated-selectors": "10.0.3",
		"postcss-for": "^2.1.1",
		"postcss-html": "^1.8.1",
		"postcss-import": "^16.1.1",
		"postcss-nesting": "^13.0.2",
		"postcss-preset-env": "^10.6.1",
		"postcss-random": "^1.0.9",
		"postcss-reporter": "7.1.0",
		"prettier": "^3.8.1",
		"prettier-plugin-astro": "^0.14.1",
		"prettier-plugin-tailwindcss": "^0.7.2",
		"purgecss": "^7.0.2",
		"rollup-plugin-analyzer": "^4.0.0",
		"rollup-plugin-visualizer": "^6.0.5",
		"semantic-release": "25.0.2",
		"stylelint": "^16.26.1",
		"stylelint-config-html": "^1.1.0",
		"stylelint-config-hudochenkov": "^12.0.1",
		"stylelint-config-standard": "^39.0.1",
		"stylelint-config-tailwindcss": "^1.0.1",
		"tailwindcss": "^4.1.18",
		"tailwindcss-fluid-type": "^2.0.7",
		"typescript": "^5.9.3"
	}
}
